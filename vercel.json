{
	"rewrites": [
		{
			"source": "/api/generate_params",
			"destination": "/api/generate_params.py"
		},
		{
			"source": "/(.*)",
			"destination": "/public/$1"
		}
	],
	"headers": [
		{
			"source": "/api/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				},
				{
					"key": "Access-Control-Allow-Methods",
					"value": "POST, OPTIONS"
				},
				{
					"key": "Access-Control-Allow-Headers",
					"value": "Content-Type, Multipart/form-data"
				}
			]
		}
	],
	"functions": {
		"api/generate_params.py": {
			"runtime": "@vercel/python@3.10",
			"memory": 1024,
			"maxDuration": 10
		}
	}
}
