{
	"routes": [
		{
			"src": "/api/generate_params",
			"dest": "/api/generate_params.py"
		},
		{
			"src": "/(.*)",
			"dest": "/public/$1"
		}
	],
	"headers": [
		{
			"source": "/api/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				},
				{
					"key": "Access-Control-Allow-Methods",
					"value": "POST, OPTIONS"
				},
				{
					"key": "Access-Control-Allow-Headers",
					"value": "Content-Type, Multipart/form-data"
				}
			]
		}
	],
	"functions": {
		"api/generate_params.py": {
			"runtime": "python3.10",
			"memory": 1024,
			"maxDuration": 10
		}
	}
}
